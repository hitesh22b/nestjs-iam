{
  "name": "@hitesh22b/nestjs-iam",
  "version": "0.0.2",
  "description": "The awesome identity access management library.",
  "author": "Hitesh Balwani",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "docs": "composedocs -d ./docs",
    "format": "npm run lint:fix && prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .js,.ts src && tsc --project tsconfig.json",
    "lint:fix": "eslint --fix --ext .js,.ts src && tsc --project tsconfig.json",
    "release": "standard-version",
    "start:dev": "tsc -w",
    "test:clear": "jest --clearCache",
    "test": "npm run test:cov",
    "posttest": "allure generate --clean --output docs/tests/report",
    "test:unit": "npm run test:clear && jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --coverageDirectory ../coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/hitesh22b/nestjs-iam#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@hitesh22b"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@types/jsonwebtoken": "^8.5.8",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "allure-commandline": "^2.13.8",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-allure": "^0.1.3",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "standard-version": "^9.3.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  }
}
